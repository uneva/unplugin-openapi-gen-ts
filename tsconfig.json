{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "types": [
            "node"
        ],
        "strict": true,
        "noUnusedLocals": true,
        "noUncheckedIndexedAccess": true,
        "declaration": true,
        "noEmit": true,
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "include": [
        "**/*.ts",
        "**/*.js"
    ]
}
